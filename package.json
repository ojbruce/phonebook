{
  "name": "phonebook",
  "version": "1.0.0",
  "description": "A simple phonebook.",
  "main": "webpack.config.js",
  "scripts": {
    "build-watch": "webpack --watch --config webpack.dev.js",
    "build": "webpack --config webpack.dev.js",
    "build-prod": "webpack -p --config webpack.prod.js",
    "test": "nyc mocha $(find src -name '*.spec.js') --require babel-core/register --require ignore-styles",
    "lint": "eslint src"
  },
  "babel": {
    "presets": [
      "react",
      "babel-preset-env"
    ]
  },
  "nyc": {
    "include": [
      "src/*"
    ],
    "extension": [
      ".js"
    ],
    "sourceMap": true,
    "instrument": true,
    "lines": 80,
    "statements": 80,
    "branches": 80,
    "functions": 80
  },
  "author": "Olivia Bruce",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "chai": "^4.1.2",
    "clean-webpack-plugin": "^0.1.17",
    "css-loader": "^0.28.8",
    "enzyme": "^3.3.0",
    "eslint": "^4.15.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-config-standard-react": "^5.0.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.5.1",
    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "html-webpack-plugin": "^2.30.1",
    "ignore-styles": "^5.0.1",
    "jsdom": "^11.5.1",
    "mocha": "^4.1.0",
    "node-sass": "^4.7.2",
    "nyc": "^11.4.1",
    "react-test-renderer": "^16.2.0",
    "sass-loader": "^6.0.6",
    "sinon": "^4.1.6",
    "style-loader": "^0.19.1",
    "uglifyjs-webpack-plugin": "^1.1.6",
    "webpack": "^3.10.0",
    "webpack-merge": "^4.1.1"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2"
  }
}
